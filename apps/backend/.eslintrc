{
  "root": true,
  "env": { "node": true },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:import/recommended",
    "plugin:import/typescript",
    "prettier"
  ],
  "rules": {
    "import/order": [
      "warn",
      { "newlines-between": "always", "alphabetize": { "order": "asc" } }
    ]
  }
}
